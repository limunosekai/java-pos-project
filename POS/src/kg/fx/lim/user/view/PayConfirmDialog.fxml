<AnchorPane prefHeight="150.0" prefWidth="300.0" style="-fx-background-color: #3d6c6f;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kg.fx.lim.user.view.PayConfirmDialogController">
   <children>
      <Label layoutX="16.0" layoutY="20.0" text="정말 결제하시겠습니까?" textFill="#fbefdf">
